});&#13;
&#13;
&#13;
for (var i = 0; i &lt; json.length; i++) {&#13;
  var block = json[i];&#13;
  var blockName = block.name;&#13;
  var blockContents = block.reportBlocksContents;&#13;
&#13;
  if (block.isActive) {&#13;
&#13;
    if (blockContents.length > 0) {&#13;
      blockContents.sort(function (a, b) {&#13;
        return (a.priorityNumber || 0) - (b.priorityNumber || 0);&#13;
      });&#13;
    }&#13;
&#13;
    for (var j = 0; j &lt; blockContents.length; j++) {&#13;
      var content = blockContents[j];&#13;
      var contentType = content.type;&#13;
      var contentTemplate = JSON.parse(content.template);&#13;
&#13;
      if (contentType === 'table') {&#13;
        var columns = contentTemplate.columns;&#13;
        var rows = content.reportBlocksContentData;&#13;
&#13;
&#13;
        tableHTML += '&lt;table border="1" bordercolor="#000000" cellspacing="0" cellpadding="10" width="100%" style="font-size : 12px; font-family : Times New Roman;">';&#13;
        tableHTML += '&lt;thead>';&#13;
        tableHTML += '&lt;tr>&lt;th  colspan="' + columns.length + '" style="text-align: center;">&lt;b>' + contentTemplate.name + '&lt;/b>&lt;/th>&lt;/tr>';&#13;
        tableHTML += '&lt;tr>';&#13;
&#13;
        for (var k = 0; k &lt; columns.length; k++) {&#13;
          tableHTML += '&lt;th>&lt;b>' + columns[k].name + '&lt;/b>&lt;/th>';&#13;
        }&#13;
&#13;
        tableHTML += '&lt;/tr>&lt;/thead>';&#13;
        tableHTML += '&lt;tbody>';&#13;
&#13;
        for (var l = 0; l &lt; rows.length; l++) {&#13;
          tableHTML += '&lt;tr>';&#13;
&#13;
          for (var m = 0; m &lt; columns.length; m++) {&#13;
            var dataIndex = columns[m].index;&#13;
            var rowData = JSON.parse(rows[l].data);&#13;
            var data;&#13;
            for (var n = 0; n &lt; rowData.rows.length; n++) {&#13;
              if (rowData.rows[n].index === dataIndex) {&#13;
                data = rowData.rows[n]&#13;
                break;&#13;
              }&#13;
            }&#13;
&#13;
            tableHTML += '&lt;td>' + data.data + '&lt;/td>';&#13;
          }&#13;
&#13;
          tableHTML += '&lt;/tr>';&#13;
        }&#13;
&#13;
        tableHTML += '&lt;/tbody>&lt;/table>&lt;br>&lt;br>';&#13;
      } else if (contentType === 'text') {&#13;
        var textData = JSON.parse(content.reportBlocksContentData[0].data);&#13;
        tableHTML += textData.data + '&lt;br>';&#13;
      } else if (contentType === 'reference') {&#13;
        var contentTemplate = JSON.parse(content.template);&#13;
        switch (contentTemplate.source) {&#13;
          case 'working_group':&#13;
            tableHTML += '&lt;h2>&lt;span>' + contentTemplate.name + '&lt;/span>&lt;/h2>&lt;br>&lt;br>'&#13;
            tableHTML += '&lt;table border="1" bordercolor="#000000" cellspacing="0" cellpadding="10" width="100%" style="font-size : 11px; font-family : Times New Roman;">';&#13;
            tableHTML += '&lt;tr>';&#13;
            for (var c = 0; c &lt; contentTemplate.columns.length; c++) {&#13;
              tableHTML += '&lt;th>&lt;b>' + contentTemplate.columns[c].name + '&lt;/b>&lt;/th>';&#13;
            }&#13;
            // tableHTML += '&lt;th>&lt;b>Working group name&lt;/b>&lt;/th>';&#13;
            // tableHTML += '&lt;th>&lt;b>Name of the nominated person&lt;/b>&lt;/th>';&#13;
            // tableHTML += '&lt;th>&lt;b>e-mail&lt;/b>&lt;/th>';&#13;
            tableHTML += '&lt;/tr>';&#13;
&#13;
            for(var wg = 0; wg &lt; wgs.length; wg++) {&#13;
              tableHTML += '&lt;tr>';&#13;
              for (var c = 0; c &lt; contentTemplate.columns.length; c++) {&#13;
                tableHTML += '&lt;th>' + wgs[wg][contentTemplate.columns[c].index] + '&lt;/th>';&#13;
              }&#13;
              // tableHTML += '&lt;td>' + (wgs[wg]['type']) + '&lt;/td>';&#13;
              // tableHTML += '&lt;td>' + (wgs[wg]['country_representative_first_name'] + ' ' + wgs[wg]['country_representative_last_name']) + '&lt;/td>';&#13;
              // tableHTML += '&lt;td>' + (wgs[wg]['country_representative_mail']) + '&lt;/td>';&#13;
              tableHTML += '&lt;/tr>';&#13;
            }&#13;
            tableHTML += '&lt;/table>';&#13;
            break;&#13;
        }&#13;
      }&#13;
    }&#13;
  }&#13;
}&#13;
