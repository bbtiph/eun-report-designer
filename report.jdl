entity EventReferences{
    name String,
    type String
}

entity EventReferencesParticipantsCategory {
    category String,
    participantsCount Long
}

relationship OneToMany {
    EventReferences to EventReferencesParticipantsCategory{eventReference(id)},
}


dto EventReferences, EventReferencesParticipantsCategory with mapstruct
service EventReferences, EventReferencesParticipantsCategory with serviceImpl


