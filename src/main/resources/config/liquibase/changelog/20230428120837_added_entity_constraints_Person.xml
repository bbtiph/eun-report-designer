<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Person.
    -->
    <changeSet id="20230428120837-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="eun_team_member_id"
                                 baseTableName="person"
                                 constraintName="fk_person__eun_team_member_id"
                                 referencedColumnNames="id"
                                 referencedTableName="eun_team_member"/>

        <addForeignKeyConstraint baseColumnNames="event_participant_id"
                                 baseTableName="person"
                                 constraintName="fk_person__event_participant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event_participant"/>

        <addForeignKeyConstraint baseColumnNames="person_in_organization_id"
                                 baseTableName="person"
                                 constraintName="fk_person__person_in_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person_in_organization"/>

        <addForeignKeyConstraint baseColumnNames="person_in_project_id"
                                 baseTableName="person"
                                 constraintName="fk_person__person_in_project_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person_in_project"/>
    </changeSet>
</databaseChangeLog>
